dark\n  >\n    <v-card-text>\n      <v-row class=\"justify-center mt-2 mb-7\">\n        <avatar/>\n      </v-row>\n      <v-row class=\"mb-4 mt-3 justify-center\">\n        <h1>\n          In√©s <span class=\"light-blue--text text--lighten-3\">Marcarian</span>\n        </h1>\n      </v-row>\n      <v-row class=\"justify-center\">\n        <v-col cols=\"11\">\n          <p class=\"body-1 mt-2 text-center mx-3\">\n            {{ $t(\"message.career\") }}\n          </p>\n        </v-col>\n      </v-row>\n\n      <sidebar-section :options=\"sections.info\" />\n      <sidebar-section :options=\"sections.socials\" />\n      <sidebar-section :options=\"sections.hobbies\">\n        <template v-slot:item=\"{item}\">\n          <v-chip class=\"ma-2\">\n            <v-icon left>\n              {{ item.icon }}\n            </v-icon>\n            {{ item.text }}\n          </v-chip>\n        </template>\n      </sidebar-section>\n      <sidebar-section :options=\"sections.languages\">\n        <template v-slot:items=\"{items}\">\n          <v-container pa-0>\n            <v-row\n              class=\"text-xs-center justify-center\"\n            >\n              <template\n                v-for=\"(item, i) in items\"\n              >\n                <v-col\n                  :key=\"i\"\n                >\n                  <v-progress-circular\n                    rotate=\"360\"\n                    size=\"90\"\n                    width=\"2\"\n                    :value=\"item.value\"\n                    color=\"white\"\n                    class=\"ma-2\"\n                  >\n                    {{ item.text }}\n                  </v-progress-circular>\n                </v-col>\n              </template>\n            </v-row>\n          </v-container>\n        </template>\n      </sidebar-section>\n    </v-card-text>\n  </v-card>\n</template>\n\n<script>\nimport Avatar from '@/views/dark-template/sidebar/Avatar'\nimport SidebarSection from '@/views/dark-template/sidebar/Sect