 in jobs\"\n          :key=\"i\"\n        >\n          <v-col cols=\"12\">\n            <h3>{{ job.title }}</h3>\n            <p class=\"mt-2\"><i>{{ job.time }}</i></p>\n            <p style=\"margin-top: -5px\">{{ job.description }}</p>\n          </v-col>\n        </v-row>\n      </content-section>\n      <content-section\n        v-if=\"educations\"\n        :title=\"$t('message.education')\"\n      >\n        <v-row\n          v-for=\"(education, i) in educations\"\n          :key=\"i\"\n          class=\"mb-2\"\n        >\n          <v-col cols=\"12\" md=\"4\">\n            <h3 class=\"font-weight-regular\">{{ education.from }} - {{ education.to }}</h3>\n          </v-col>\n          <v-col cols=\"12\" md=\"8\">\n            <h3 v-if=\"education.title\">{{ education.title }}</h3>\n            <div v-if=\"education.location\">\n              <i>{{ education.location }}</i>\n            </div>\n            <div v-if=\"education.description\">\n              {{ education.description }}\n            </div>\n          </v-col>\n          <v-col>\n            <v-divider></v-divider>\n          </v-col>\n        </v-row>\n      </content-section>\n      <content-section\n        v-if=\"skills\"\n        id=\"to-timeline\"\n        :title=\"$t('message.skills')\"\n      >\n        <template slot=\"actions\">\n          <p class=\"font-weight-regular caption\">({{ $t('message.percentage') }})</p>\n        </template>\n        <v-row wrap>\n          <template\n            v-for=\"(skill, i) in skills\"\n          >\n            <v-col\n              v-if=\"skill.divider\"\n              :key=\"i\"\n              cols=\"12\"\n              mb-4\n            />\n            <v-col\n              v-else\n              :key=\"i\"\n              md=\"6\"\n              cols=\"12\"\n            >\n              <div\n                class=\"mr-2 ml-2\"\n              >\n                <div class=\"align-center\">\n                  <v-icon\n                    small\n                  >\n                    {{ skill.icon }}\n                  </v-icon>\n                  {{ skill.title }}\n                </div>\n                <v-progress-linear\n                    color=\"blue darken-3\"\n                    height=\"3\"\n                    :value=\"skill.value\"\n                    rounded\n                />\n              </div>\n            </v-col>\n          </template>\n        </v-row>\n      </content-section>\n    </v-card-text>\n  </v-card>\n</template>\n\n<script>\nimport ContentSection from '@/views/dark-template/content/Section'\nimport {global} from \"@/plugins/i18n\";\n\nexport default {\n  name      : 'MainContent',\n  components: { ContentSection },\n  data      : () => ({\n    skills: global.skills_items\n  }),\n  computed: {\n    educations() {\n      return [\n        this.$t(\"message.school\"),\n        this.$t(\"message.university\")\n      ]\n    },\n    jobs() {\n      return [\n        this.$t(\"message.website_job\")\n      ]\n    }\n  }\n}\n</script>\n\n<style scoped>\n.title {\n  border-bottom: 2px #bfbfbf solid;\n  line-height: 1.5 !important;\n}\n.progress {\n  margin-top: 0.1rem;\n}\n</style>\n"]}]}